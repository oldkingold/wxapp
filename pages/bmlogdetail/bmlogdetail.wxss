page {
  height: 100%;
}

view,text {
  font-family:PingFangSC-Light,helvetica,'Heiti SC';
  font-size:29rpx;
  color:#333;
}

.container {
  background: #f5f5f5;
  height: 100%;
  padding-top: 30rpx;
}

::-webkit-scrollbar {
  width: 0px;
  height: 0px;
  background-color: transparent;
}

.content {
  background-color: #fff;
  border-radius: 18rpx;
  box-shadow: 2rpx 2rpx 2rpx #c8c9cb;
  margin-left: 30rpx;
  margin-right: 30rpx;
  padding: 16rpx 6rpx 40rpx 23rpx;
  overflow: hidden;
  position: relative;
}
.item{
  /* overflow: hidden; */
  
}
.content-line {
  display: flex;
  line-height: 50rpx;
}

.title {
  font-size: 30rpx;
  font-weight: 600;
}

.object-name {
  width: 160rpx;
  margin-right: 18rpx;
}

.attend-right {
  display: flex;
  flex-wrap: wrap;
  width: 73%;
}

.place {
  margin-left: 18rpx;
}

.middle {
  background-color: #eef2f3;
  border-radius: 18rpx;
  padding-left: 18rpx;
  padding-right: 18rpx;
  margin-top: 16rpx;
  margin-right: 8px;
  padding: 16rpx 6rpx 20rpx 23rpx;
}

.invoice-image-object {
  position: relative;
}

.trangle {
  width: 20rpx;
  height: 16rpx;
  top: -29rpx;
  position: absolute;
  left: 8px;
}

.bottom-object {
  margin-top: 40rpx;
  height: 80rpx;
  display: flex;
  justify-content: space-around;
  margin-right:30rpx;
  margin-left:30rpx;
}

.canclebm {
  display: flex;
  align-items: center;
  justify-content: center;
   width: 40%; 
  background-color: #c8c9cb;
  border: 1rpx solid #c8c9cb;
  border-radius: 47rpx;
  font-weight: 600;
  letter-spacing: 2rpx;
}

.changebm {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 40% ;
  background-color: #2185f7;
  border: 1rpx solid #2185f7;
  border-radius: 47rpx;
  color: #fff;
  font-weight: 600;
  letter-spacing: 2rpx;
}

.is-over-time{
  color: #BBBBBB
}
.m-status-img{
  position:absolute;
  top: 60rpx;
  right: 0rpx;
}
.m-status-img image{
width:200rpx;
height:200rpx;
position:absolute;
top:-92rpx;
right:-32rpx;
}