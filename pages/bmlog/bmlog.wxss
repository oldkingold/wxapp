page {
  height: 100%;
}

view,text {
  font-family:PingFangSC-Light,helvetica,'Heiti SC';
  font-size:29rpx;
  color:#333;
}

.container {
  background: #f5f5f5;
  height: 100%;
}

::-webkit-scrollbar {
  width: 0px;
  height: 0px;
  background-color: transparent;
}

.scoll-h {
  height: 100%;
}

.swiper-content {
  overflow-y: auto;
  height: 94%;
}

.cate-nav {
  width: inherit;
  background-color: #fff;
  height: 6%;
}

.tab-h {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  height: 80rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 80rpx;
  font-size: 16px;
  white-space: nowrap;
  z-index: 99;
}

.item {
  background-color: #fff;
  border-radius: 18rpx;
  box-shadow: 2rpx 2rpx 2rpx #c8c9cb;
  padding: 16rpx 6rpx 0rpx 18rpx; 
  margin: 35rpx 25rpx 5rpx 25rpx;
  overflow: hidden;
  position: relative;
}

/* .content {
  margin-top: 12rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
} */

.content-line {
  display: flex;
  line-height: 50rpx;
}

.object-name {
  width: 134rpx;
  margin-right: 18rpx;
}

.title {
  font-size: 30rpx;
  font-weight: 600;
}

.line-text {
  font-weight: 600;
}

.place {
  margin-left: 18rpx;
}

.attend-right {
  display: flex;
  flex-wrap: wrap;
  width: 73%;
}

.content-bottom {
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  height: 80rpx;
  border-top: 1rpx solid #f4f4f4;
  align-items: center;
}

.changebm {
  color: #0d7ef6;
  border: 1rpx solid #0d7ef6;
  border-radius: 22rpx;
  width: 146rpx;
  text-align: center;
  margin-right: 18rpx;
}

.canclebm {
  color: #9e9e9e;
  border: 1rpx solid #9e9e9e;
  border-radius: 22rpx;
  width: 146rpx;
  text-align: center;
  margin-right: 18rpx;
}

.rebm{
    color: #FF8E8E;
  border: 1rpx solid #FF8E8E;
  border-radius: 22rpx;
  width: 146rpx;
  text-align: center;
  margin-right: 18rpx;
}

.tab-item {
  margin: 0 36rpx;
  display: inline-block;
  font-weight: 600;
  font-size: 30rpx;
  letter-spacing: 2rpx;
  color: #1b1b1b;
}

.changebm-is-over-time {
  color: #848484;
  border-radius: 22rpx;
  width: 146rpx;
  text-align: center;
  margin-right: 18rpx;
  background-color: #E9E9E9
}

.canclebm-is-over-time {
  color: #848484;
  border-radius: 22rpx;
  width: 146rpx;
  text-align: center;
  margin-right: 18rpx;
   background-color: #E9E9E9
}

.is-over-time{
  color: #BBBBBB
}

.tab-item.active {
  color: #0d7ef6;
  position: relative;
  font-weight: 600;
  font-size: 30rpx;
  letter-spacing: 2rpx;
}

.tab-item.active:after {
  content: "";
  display: block;
  height: 4rpx;
  width: 82rpx;
  background: #0d7ef6;
  position: absolute;
  bottom: 5rpx;
  left: 8rpx;
  border-radius: 16rpx;
}

.m-status-img{
  position:absolute;
  top: 60rpx;
  right: 0rpx;
}

.m-status-img image{
  width:200rpx;
  height:200rpx;
  position:absolute;
  top:-92rpx;
  right:-32rpx;
}

.nolog{
  margin-top:30%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center
}

.nolog image{
 width: 389rpx;
 height: 287rpx;
 display: block;
 margin: 10rpx auto;
}

.nolog-text{
  color: #A0A1A3
}
